<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>slot</display-name>
   
   <servlet>
   	<servlet-name>CreateDialog</servlet-name>
   	<servlet-class>
   		com.puzhen.slot.servlet.DialogCreationServlet
   	</servlet-class>
   </servlet>
   
   <servlet-mapping>
   	<servlet-name>CreateDialog</servlet-name>
   	<url-pattern>/createDialog</url-pattern>
   </servlet-mapping>
   
   <servlet>
   	<servlet-name>Dialog</servlet-name>
   	<servlet-class>
   		com.puzhen.slot.servlet.DialogServlet
   	</servlet-class>
   </servlet>
   
   <servlet-mapping>
   	<servlet-name>Dialog</servlet-name>
   	<url-pattern>/dialog</url-pattern>
   </servlet-mapping>

   <servlet>
      <servlet-name>Dummy</servlet-name>
      <servlet-class>
         com.puzhen.slot.servlet.DummyServlet
      </servlet-class>
   </servlet>

   <servlet-mapping>
      <servlet-name>Dummy</servlet-name>
      <url-pattern>/dummy</url-pattern>
   </servlet-mapping>
</web-app>